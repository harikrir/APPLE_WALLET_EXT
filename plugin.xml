<?xml version="1.0" encoding="UTF-8"?>
<plugin id="com.kfh.wallet.issuer" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0">
<name>KFH Wallet Extension</name>
<platform name="ios">
<dependency id="cordova-plugin-add-app-extension" url="https://github.com/dpa99c/cordova-plugin-add-app-extension.git" />
<config-file target="*/Entitlements-Release.plist" parent="com.apple.security.application-groups">
<array><string>group.com.bhuat.wallet</string></array>
</config-file>
<extension-target name="KFHWalletHandler" identifier="com.kfh.bh.wallet.ext" type="issuer-provisioning-non-ui">
<source-file src="src/ios/KFHWalletHandler.swift" />
</extension-target>
</platform>
</plugin>

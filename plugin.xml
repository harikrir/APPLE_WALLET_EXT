<?xml version='1.0' encoding='utf-8'?>
<plugin id="kfh-wallet-plugin" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
<name>KFH Wallet Extension</name>
<description>KFH Apple Wallet Provisioning Plugin with UI and Non-UI Extensions</description>
<license>Apache 2.0</license>
<keywords>cordova,apple,wallet,provisioning</keywords>
<platform name="ios">
<config-file target="config.xml" parent="/*">
<feature name="KFHWalletPlugin">
<param name="ios-package" value="KFHWalletPlugin" />
</feature>
</config-file>
<config-file target="*/Entitlements-Release.plist" parent="com.apple.security.application-groups">
<array>
<string>group.com.aub.mobilebanking.uat.bh</string>
</array>
</config-file>
<config-file target="*/Entitlements-Debug.plist" parent="com.apple.security.application-groups">
<array>
<string>group.com.aub.mobilebanking.uat.bh</string>
</array>
</config-file>
<extension-target name="KFHNonUI" bundle-id="com.aub.mobilebanking.uat.bh.KFHNonUI">
<config-file target="*-Info.plist" parent="NSExtension">
<dict>
<key>NSExtensionPointIdentifier</key>
<string>com.apple.PassKit.issuer-provisioning</string>
<key>NSExtensionPrincipalClass</key>
<string>KFHWalletHandler</string>
</dict>
</config-file>
<config-file target="*/Entitlements-Release.plist" parent="com.apple.security.application-groups">
<array><string>group.com.aub.mobilebanking.uat.bh</string></array>
</config-file>
<config-file target="*/Entitlements-Debug.plist" parent="com.apple.security.application-groups">
<array><string>group.com.aub.mobilebanking.uat.bh</string></array>
</config-file>
</extension-target>
<extension-target name="KFHUI" bundle-id="com.aub.mobilebanking.uat.bh.KFHUI">
<config-file target="*-Info.plist" parent="NSExtension">
<dict>
<key>NSExtensionPointIdentifier</key>
<string>com.apple.PassKit.issuer-provisioning.authorization</string>
<key>NSExtensionPrincipalClass</key>
<string>KFHUIHandler</string>
</dict>
</config-file>
<config-file target="*/Entitlements-Release.plist" parent="com.apple.security.application-groups">
<array><string>group.com.aub.mobilebanking.uat.bh</string></array>
</config-file>
<config-file target="*/Entitlements-Debug.plist" parent="com.apple.security.application-groups">
<array><string>group.com.aub.mobilebanking.uat.bh</string></array>
</config-file>
</extension-target>
<header-file src="src/ios/KFHWalletPlugin.swift" />
<source-file src="src/ios/KFHWalletPlugin.swift" />
<source-file src="src/ios/KFHWalletHandler.swift" />
<source-file src="src/ios/KFHUIHandler.swift" />
<resource-file src="src/ios/kfh_card_art.png" />
<framework src="PassKit.framework" />
<framework src="UIKit.framework" />
</platform>
</plugin>
 

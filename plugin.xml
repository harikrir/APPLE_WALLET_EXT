<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" id="com.aub.mobilebanking.wallet" version="1.0.0">
<name>AUB Wallet Plugin</name>
<description>AUB Apple Wallet In-App Provisioning Plugin for OutSystems MABS</description>
<js-module src="www/AUBWallet.js" name="AUBWallet">
<clobbers target="AUBWallet" />
</js-module>
<platform name="ios">
<hook type="before_plugin_install" src="hooks/MoveExtensionsFolders.js"/>
<hook type="after_prepare" src="hooks/configure_extensions.js"/>
<config-file target="config.xml" parent="/*">
<feature name="AUBWalletPlugin">
<param name="ios-package" value="AUBWalletPlugin" />
</feature>
</config-file>
<source-file src="src/ios/AUBWalletPlugin.swift" />
<resource-file src="src/ios/kfh_card_art.png" />
<config-file target="*/Entitlements-Debug.plist" parent="com.apple.developer.payment-pass-provisioning">
<true/>
</config-file>
<config-file target="*/Entitlements-Release.plist" parent="com.apple.developer.payment-pass-provisioning">
<true/>
</config-file>
<config-file target="*/Entitlements-Debug.plist" parent="com.apple.security.application-groups">
<array>
<string>group.com.aub.mobilebanking.uat.bh</string>
</array>
</config-file>
<config-file target="*/Entitlements-Release.plist" parent="com.apple.security.application-groups">
<array>
<string>group.com.aub.mobilebanking.uat.bh</string>
</array>
</config-file>
<framework src="PassKit.framework" />
<framework src="SwiftUI.framework" />
<framework src="LocalAuthentication.framework" />
<preference name="SwiftVersion" value="5.0" />
</platform>
</plugin>



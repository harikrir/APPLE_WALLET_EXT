<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" id="cordova-plugin-wallet-extension" version="1.0.0">
    <name>WalletExtensionPlugin</name>
    <description>Cordova plugin for Wallet Extensions</description>
    <license>MIT</license>
    <keywords>wallet, extension, passkit</keywords>
    <js-module src="www/WalletExtensionPlugin.js" name="WalletExtensionPlugin">
        <clobbers target="WalletExtensionPlugin" />
    </js-module>
    <platform name="ios">
        <config-file target="config.xml" parent="/*">
            <feature name="WalletExtensionPlugin">
                <param name="ios-package" value="WalletExtensionPlugin" />
            </feature>
        </config-file>
        <source-file src="src/ios/WalletExtensionPlugin.h" />
        <source-file src="src/ios/WalletExtensionPlugin.m" />
        <framework src="PassKit.framework" />
    </platform>
</plugin>
